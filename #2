using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int x = 1;
            int xn = 0;
            {
                for (int i = 0; i < 10; i++)
                {
                    int m = xn + x;
                    Console.WriteLine(m);
                    xn = x;
                    x = m;
                }
                Console.ReadKey();
            }
        } 
    }
}
